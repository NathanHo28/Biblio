<div>
	<% @user.stories.each do |story| %>
		<%= story.title %>
		<% story.pages.each do |page| %>
		<div class="clickable_image">
			<%= link_to image_tag(page.page_photo_url(:medium)), page.page_photo_url(:medium), class: "fancybox", :rel => story.title, :title => page.caption %>
		</div>
		<div class="likes">
			<p>Likes: <%= story.score %></p>
		  <p><%= link_to "upvote", like_story_path(story), method: :put %></p>
		<% end %>
	<% end %>
</div>